{
  "name": "toni",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "description": "toni",
  "scripts": {
    "start": "node dist/app.js",
    "start2": "ts-node-dev --no-notify --respawn --transpile-only src/app",
    "clean": "rm -rf dist",
    "build": "rm -rf build && npm i && npm run generate:prisma && tsc",
    "generate": "yarn pal g && npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/nexusSchema",
    "db-dev": "prisma migrate dev",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "dev2": "ts-node-dev --no-notify --respawn --transpile-only src/app"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "generator": {
      "client": {
        "provider": "prisma-client-js",
        "config": {
          "binaryTargets": [
            "native",
            "rhel-openssl-1.0.x",
            "rhel-openssl-1.1.x"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@apollo/server": "^4.5.0",
    "@paljs/nexus": "^6.0.7",
    "@paljs/types": "^6.0.7",
    "@prisma/client": "5.5.2",
    "@types/multer": "^1.4.7",
    "apollo-server": "3.12.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.36.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-shield": "^7.6.5",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nexus": "1.3.0",
    "nodemon": "3.0.1",
    "stripe": "^12.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@paljs/cli": "^6.0.7",
    "@types/node": "18.15.3",
    "prettier": "2.8.4",
    "prisma": "^5.5.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "repository": "",
  "author": "toni"
}
